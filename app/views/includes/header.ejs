    <div class="page-header header-filter clear-filter purple-filter" data-parallax="true" style="background-image: url('assets/img/kit/bg2.jpg');">
        <div class="container">
            <div class="row">
                <div class="col-md-8 ml-auto mr-auto">
                    <div class="brand">
                        <img src="img/logo-white-content.png" alt="">
                        <h3>INOVAÇÃO SEM LIMITES</h3>
                    </div>
                    <div class="form-group">
                        <div class="input-group">
                            <div class="input-group-prepend">
                                <span class="input-group-text">
                                    <i class="material-icons">search</i>
                                </span>
                            </div>
                            <input type="text" class="form-control" placeholder="Qual cidade você procura?">
                            <%
                                if(results){
                                    if(list.length > 0){
                            %>
                            <b>Resultados da pesquisa por <%= search %>:</b>
                            <ul>
                                <% list.forEach(function(item){ %>
                                    <li><%= item.name %></li>
                                <% }); %>
                            </ul>
                            <% } else{ %>
                                <b>Nenhum resultado encontrado para <%= search %>!</b>
                            <% 
                                }
                            }
                            %>
                        </div>
                    </div>
                </div>
            </div>
        </div>
